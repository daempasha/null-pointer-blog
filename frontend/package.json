    "react-icons": "^4.4.0"
  },
  "devDependencies": {
    "@sanity/cli": "^2.33.2",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "18.6.3",
